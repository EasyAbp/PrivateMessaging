@page
@inherits EasyAbp.PrivateMessaging.Web.Pages.PrivateMessagingPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EasyAbp.PrivateMessaging.Web.Pages.PrivateMessages.PrivateMessage.EditModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="PrivateMessage" data-ajaxForm="true" asp-page="/PrivateMessages/PrivateMessage/EditModal">
    <abp-modal>
        <abp-modal-header title="@L["EditPrivateMessage"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>